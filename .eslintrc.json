{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "plugin:vue/recommended"
    ],
    "rules": {
        "indent": "off",
        "vue/script-indent": ["error", 4, {
            "baseIndent": 1
          }]
    },
    "parserOptions": {
        "parser": "babel-eslint"
    }
}